<table class="table table-active table-condensed" id="myTable">
  <thead>
    <tr>
      <th scope="col">Trip Name</th>
      <th scope="col">From </th>
      <th scope="col">To</th>
    </tr>
  </thead>
  <tbody class="panel">
    {{#each trips}}
    <tr class="trip-data">
      <th><button type="button" class="btn btn-comm get-destinations-all" value={{id}}><i class="fa fa-comments pr-1"></i>
          {{tripName}}</button></th>

      <td>{{tripStartDate}}</td>
      <td>{{tripEndDate}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
<div class="col" id="destinations-table" style="width:100%;margin:0px"></div>



<div class="row mt-5">
  <div class="col-12">

    <div class="card">
      <div class="card-header">
        <h2>My Reviews</h2>
      </div>
      <ul class="list-group">
        {{#each reviews}}
        <li class="list-group-item">
          {{titleCategory}} &emsp;
          {{rating}}
        </li>
        {{/each}}

      </ul>
    </div>

  </div>
</div>