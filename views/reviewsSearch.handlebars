
  <table class="table table-bordered">
  <thead>
    <tr>
      
      <th scope="col">Review Name</th>
      <th scope="col">Rating</th>
      <th scope="col">Comments</th>
    </tr>
  </thead>
  <tbody>
     {{#each review}}
    <tr>
     <td>{{titleCategory}}</td>
      <td>{{rating}} / 5</td>
      <td>{{review}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

  
  
  <div class="row">
    


  <div class="col-3">
    <h2>Review Title</h2>
    <ul class="list-group">
      {{#each review}}
      <li class="list-group-item">
        {{titleCategory}}
      </li>
      {{/each}}
    </ul>
  </div>

  <div class="col-2">
    <h2>Rating</h2>
    <ul class="list-group">
      {{#each review}}
      <li class="list-group-item">
        {{rating}} / 5
      </li>
      {{/each}}
    </ul>
  </div>

  <div class="col-2">
    <h2>Comments</h2>
    <ul class="list-group">
      {{#each review}}
      <li class="list-group-item">
        {{review}}
      </li>
      {{/each}}
    </ul>
  </div>

  <div class="col-5">